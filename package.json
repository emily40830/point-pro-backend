{
  "name": "point-pro-backend",
  "version": "1.0.0",
  "repository": "https://github.com/emily40830/point-pro-backend.git",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --exit-child --respawn --files src/server.ts",
    "add-seeds:prod": "ts-node-dev --files prisma/migrate.ts",
    "update-reservations": "ts-node-dev --files prisma/updateReservations.ts",
    "update-periods": "ts-node-dev --files prisma/updatePeriods.ts",
    "build": "tsc -p .",
    "build:prod": "prisma migrate deploy && prisma generate && tsc -p .",
    "start": "node dist/server.js",
    "makemigrations": "npx prisma migrate dev --create-only"
  },
  "prisma": {
    "seed": "ts-node prisma/migrate.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@types/multer": "^1.4.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "imgur": "^2.3.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "line-pay-merchant": "^0.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-ecpay-aio": "^0.2.3",
    "nodemailer": "^6.9.3",
    "pg": "^8.10.0",
    "ramda": "^0.29.0",
    "socket.io": "^4.6.1",
    "tslib": "^2.5.3",
    "typescript": "^5.0.3",
    "uuid": "^9.0.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.8",
    "@types/pg": "^8.6.6",
    "@types/ramda": "^0.29.2",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.2",
    "@types/yup": "^0.32.0",
    "prisma": "^4.15.0",
    "supabase": "^1.49.4",
    "ts-node-dev": "^2.0.0"
  }
